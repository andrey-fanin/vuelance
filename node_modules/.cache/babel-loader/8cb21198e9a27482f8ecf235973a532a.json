rcesContent":["<template>\n  <h1 v-if=\"tasks.length === 0\" class=\"text-white center\">No tasks here...</h1>\n  <template v-else>\n    <h3>Number of active tasks: {{ activeTasksCount }}</h3>\n    <div class=\"card\" v-for=\"task in tasks\" :key=\"task.id\">\n      <h2 class=\"card-title\">\n        {{task.title}}\n        <TaskStatus :type=\"task.status\" />\n      </h2>\n      <p>\n        <strong>\n          <small>\n            {{new Date(task.date).toLocaleDateString()}}\n          </small>\n        </strong>\n      </p>\n      <button class=\"btn primary\" @click=\"open(task.id)\">Open</button>\n    </div>\n    <button class=\"btn\" @click=\"clearTasks\">Clear the list</button>\n  </template>\n</template>\n\n<script>\nimport {computed} from 'vue'\nimport {useStore} from 'vuex'\nimport {useRouter} from 'vue-router'\nimport TaskStatus from '../components/TaskStatus'\n\nexport default {\n  name: 'Home',\n  setup() {\n    const store = useStore()\n    const router = useRouter()\n\n    function clearTasks() {\n        localStorage.removeItem('my-tasks');\n        router.go();\n    }\n\n    const tasks = computed(() => store.getters.tasks)\n    const activeTasksCount = computed(() => store.getters.activeTasksCount)\n\n    return {\n      tasks, activeTasksCount, clearTasks,\n      open: id => router.push(`/task/${id}`)\n    }\n  },\n  components: {TaskStatus}\n}\n</script>\n"],"sourceRoot":""}]}