      const newTask = {\n        id: Date.now().toString(),\n        title: title.value,\n        date: new Date(date.value).setHours(23,59,59,999),\n        description: description.value,\n        status: 'active'\n      }\n\n      store.dispatch('createTask', newTask)\n      router.push('/')\n    }\n\n    const isValid = computed(() => {\n      return title.value !== '' &&\n             date.value && description.value !== ''\n    })\n\n    return {\n      submit,\n      title,\n      date,\n      description,\n      isValid,\n    }\n  }\n}\n</script>"],"sourceRoot":""}]}