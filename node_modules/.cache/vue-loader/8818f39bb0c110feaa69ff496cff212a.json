 const id = props.id\n    const task = computed(() => store.getters.taskById(id))\n\n    const setStatus = status => {\n      const updated = {...task.value, status}\n      store.dispatch('changeTask', updated)\n    }\n\n    return {\n      task,\n      id,\n      setStatus\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}